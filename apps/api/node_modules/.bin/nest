else
  export NODE_PATH="/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/@nestjs+cli@11.0.6_@swc+cli@0.6.0_@swc+core@1.11.20_chokidar@4.0.3__@swc+core@1.11.20_@types+node@22.14.1/node_modules/@nestjs/cli/bin/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/@nestjs+cli@11.0.6_@swc+cli@0.6.0_@swc+core@1.11.20_chokidar@4.0.3__@swc+core@1.11.20_@types+node@22.14.1/node_modules/@nestjs/cli/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/@nestjs+cli@11.0.6_@swc+cli@0.6.0_@swc+core@1.11.20_chokidar@4.0.3__@swc+core@1.11.20_@types+node@22.14.1/node_modules/@nestjs/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/@nestjs+cli@11.0.6_@swc+cli@0.6.0_@swc+core@1.11.20_chokidar@4.0.3__@swc+core@1.11.20_@types+node@22.14.1/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
