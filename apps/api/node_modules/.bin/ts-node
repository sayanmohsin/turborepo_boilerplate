#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules/ts-node/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules/ts-node/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.11.20_@types+node@22.14.1_typescript@5.7.3/node_modules:/Users/sayanmohsin/Space/Programming/rd/turborepo_boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
